{
  "name": "@straw-hat/next-sdk",
  "version": "0.5.1",
  "description": "A Software Development Kit for NextJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript-monorepo.git",
    "directory": "packages/@straw-hat/next-sdk"
  },
  "license": "MIT",
  "keywords": [
    "straw-hat-team",
    "nextjs",
    "next",
    "react",
    "sdk"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "yarn run -T rimraf dist",
    "build": "yarn prebuild && yarn run -T tsc -p tsconfig.prod.json",
    "test:jest": "yarn run -T jest",
    "prepack": "yarn build",
    "docs:typedoc": "yarn run -T typedoc"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "next": "^12.0.4",
    "react": "^17.0.2"
  },
  "dependencies": {
    "@straw-hat/types": "^0.5.0"
  },
  "devDependencies": {
    "@straw-hat/tsconfig": "^3.0.0",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "jest": "^27.5.1",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
