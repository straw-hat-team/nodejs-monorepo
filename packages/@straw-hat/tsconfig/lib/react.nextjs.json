{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base",
  "display": "React Next.js",
  "compilerOptions": {
    "target": "es2022",
    "jsx": "preserve",
    "lib": ["es2022", "dom", "dom.iterable"],
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "module": "Preserve",
    "moduleResolution": "bundler",
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "verbatimModuleSyntax": true
  }
}
