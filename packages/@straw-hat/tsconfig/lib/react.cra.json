{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base",
  "display": "React CRA",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "declaration": false,
    "emitDeclarationOnly": false,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "Preserve",
    "noEmit": true
  }
}
