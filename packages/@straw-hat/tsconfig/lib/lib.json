{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base",
  "display": "Library",
  "compilerOptions": {
    "allowImportingTsExtensions": false,
    "noEmit": false,
    "emitDeclarationOnly": false,
    "module": "Node16",
    "moduleResolution": "node16",
    "target": "ES2022",
    "verbatimModuleSyntax": true,
    "composite": true
  }
}
