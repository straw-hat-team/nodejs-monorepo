{
  "name": "@straw-hat/jest-config-chain",
  "version": "1.0.3",
  "description": "Chainable configuration for Jest",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/nodejs-monorepo.git",
    "directory": "packages/@straw-hat/jest-config-chain"
  },
  "license": "MIT",
  "keywords": [
    "config",
    "chainable",
    "config-chain",
    "jest",
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "yarn run -T rimraf dist",
    "build": "yarn prebuild && yarn run -T tsc -p tsconfig.prod.json",
    "test:jest": "yarn run -T jest",
    "prepack": "yarn build",
    "docs:typedoc": "yarn run -T typedoc"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@straw-hat/chainable-config": "^0.9.0"
  },
  "devDependencies": {
    "@straw-hat/tsconfig": "^3.0.2",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
